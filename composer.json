{
    "name": "transvoucher/php-sdk",
    "description": "Official PHP SDK for TransVoucher payment processing API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "TransVoucher",
            "email": "developers@transvoucher.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "guzzlehttp/guzzle": "^7.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "symfony/var-dumper": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "TransVoucher\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TransVoucher\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "keywords": [
        "transvoucher",
        "payment",
        "crypto",
        "api",
        "sdk"
    ],
    "homepage": "https://transvoucher.com",
    "support": {
        "email": "developers@transvoucher.com",
        "docs": "https://transvoucher.com/api-documentation"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
} 